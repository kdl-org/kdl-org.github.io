  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.1.0",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "shiki": "^1.24.2",
    "tailwindcss": "^2.0.2"
  }
